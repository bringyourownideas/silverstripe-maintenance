{
    "name": "bringyourownideas/silverstripe-maintenance",
    "description": "Toolset to help with the day by day maintenance work.",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/bringyourownideas/silverstripe-maintenance",
    "keywords": [
        "silverstripe",
        "environment",
        "composer",
        "updates",
        "vulnerabilities",
        "security"
    ],
    "authors": [
        {
            "name": "Peter Thaleikis",
            "homepage": "https://releasecandidate.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5",
        "silverstripe/reports": "^5",
        "silverstripe/supported-modules": "dev-main",
        "symbiote/silverstripe-queuedjobs": "^5",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3",
        "symfony/thanks": "^1.2",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "BringYourOwnIdeas\\Maintenance\\": "src/",
            "BringYourOwnIdeas\\Maintenance\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "suggest": {
        "bringyourownideas/silverstripe-composer-update-checker": "Checks for available updates of dependencies"
    },
    "support": {
        "issues": "https://github.com/bringyourownideas/silverstripe-maintenance/issues"
    },
    "replace": {
        "friendsofsilverstripe/silverstripe-maintenance": "self.version"
    },
    "config": {
        "allow-plugins": {
            "symfony/thanks": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
